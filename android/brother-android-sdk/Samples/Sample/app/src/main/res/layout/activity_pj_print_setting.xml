                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/NumCopies_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="24dp"
                    android:layout_marginTop="14dp"
                    android:text=""
                    android:textSize="14sp" />

            </androidx.cardview.widget.CardView>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />
            <androidx.cardview.widget.CardView
                android:id="@+id/SkipStatusCheck"
                android:layout_width="match_parent"
                android:layout_height="66dp"
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                android:onClick="changeSkipStatusCheck">

                <TextView
                    android:id="@+id/SkipStatusCheck_caption"
                    android:layout_width="wrap_content"
                    android:layout_height="49dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="24dp"
                    android:text="SkipStatusCheck"
                    android:textColor="@android:color/primary_text_light"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/SkipStatusCheck_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="24dp"
                    android:layout_marginTop="14dp"
                    android:text=""
                    android:textSize="14sp" />

            </androidx.cardview.widget.CardView>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />
            <androidx.cardview.widget.CardView
                android:id="@+id/PrintQuality"
                android:layout_width="match_parent"
                android:layout_height="66dp"
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                android:onClick="changePrintQuality">

                <TextView
                    android:id="@+id/PrintQuality_caption"
                    android:layout_width="wrap_content"
                    android:layout_height="49dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="24dp"
                    android:text="PrintQuality"
                    android:textColor="@android:color/primary_text_light"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/PrintQuality_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="24dp"
                    android:layout_marginTop="14dp"
                    android:text=""
                    android:textSize="14sp" />

            </androidx.cardview.widget.CardView>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <Button
                android:id="@+id/validate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Validate" />

        </LinearLayout>
    </ScrollView>
</LinearLayout>